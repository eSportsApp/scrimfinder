generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model bannedUsers {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  userId String @unique
  reason String?
}

model guilds {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  guildId String @unique
  rssChannelId String?
  //Warmup Searches
  warmuprssChannelId String?
}


model users {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  userId String @unique
  username String?
  rssClan String?
  rssclass String?
  
  
}

model badwords{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  Word String @unique
}

model blGuilds {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  guildId String @unique
  reason String?
}
